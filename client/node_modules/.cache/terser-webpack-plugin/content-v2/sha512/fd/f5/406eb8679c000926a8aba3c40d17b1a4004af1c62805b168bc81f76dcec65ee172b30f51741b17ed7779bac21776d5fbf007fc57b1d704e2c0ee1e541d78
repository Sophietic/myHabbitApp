e nieuwe waarden uit de formstate zodat je die met service naar backend kan sturen\n    const { email, password } = formState;\n    // met service hadden we een signup methode zodat je naar de signup route van de backend kan gaan\n    service\n      .signup(email, password)\n      .then((response) => {\n        setForm(initialState);\n        props.getLoggedInUser(response);\n      })\n      .catch((error) => console.log(error));\n  }\n\n  return (\n    <section className=\"section\">\n      <div className=\"container\">\n        <div className=\"columns is-centered\">\n          <div className=\"column is-auto\">\n            <div className=\"card\">\n              <form className=\"card-content\" onSubmit={handleSubmit}>\n                <h1 className=\"title is-1 is-family-code\">Sign Up</h1>\n                <div className=\"field\">\n                  <label className=\"label is-medium\" htmlFor=\"email\">\n                    Email Address\n                  </label>\n                  <div className=\"control\">\n                    <input\n                      name=\"email\"\n                      type=\"text\"\n                      value={formState.email}\n                      onChange={handleChange}\n                      className=\"input\"\n                      placeholder=\"Please provide your email address\"\n                    />\n                  </div>\n                </div>\n\n                <div className=\"field\">\n                  <label className=\"label is-medium\" htmlFor=\"password\">\n                    Password\n                  </label>\n                  <div className=\"control\">\n                    <input\n                      name=\"password\"\n                      type=\"text\"\n                      value={formState.password}\n                      onChange={handleChange}\n                      className=\"input\"\n                      placeholder=\"Please provide your password\"\n                    />\n           